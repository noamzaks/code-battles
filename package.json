{
  "name": "code-battles",
  "version": "1.7.3",
  "description": "A library for building interactive competitive coding battles",
  "repository": "https://github.com/noamzaks/code-battles",
  "homepage": "https://code-battles.readthedocs.org",
  "author": "Noam Zaks <imnoamzaks@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./styles.css": "./dist/esm/styles.css"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "chokidar --silent \"src/**\" \"pdoc-template/**\" \"code_battles/**\" --initial -c \"rollup -c\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "prismjs": "^1.30.0",
    "react-countdown": "^2.3.6",
    "react-firebase-hooks": "^5.1.1"
  },
  "peerDependencies": {
    "@mantine/core": "^8.0.1",
    "@mantine/dates": "^8.0.1",
    "@mantine/dropzone": "^8.0.1",
    "@mantine/hooks": "^8.0.1",
    "@mantine/notifications": "^8.0.1",
    "@mantine/charts": "^8.0.1",
    "recharts": "^2.15.3",
    "dayjs": "^1.11.13",
    "firebase": "^11.7.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@pyscript/core": "^0.6.52",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "chokidar-cli": "^3.0.0",
    "prettier": "^3.5.3",
    "rollup": "^4.41.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-import-css": "^3.5.8",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "prettier": {
    "semi": false,
    "endOfLine": "crlf"
  }
}
